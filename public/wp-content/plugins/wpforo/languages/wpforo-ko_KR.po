msgstr "이름 표시 Z->A"

#: classes/Settings.php:1079
msgid "Display Name A->Z"
msgstr "이름 표시 A->Z"

#: classes/Settings.php:1078
msgid "Join Date Descending"
msgstr "가입 날짜 내림차순"

#: classes/Settings.php:1077
msgid "Join Date Ascending"
msgstr "가입 날짜 오름차순"

#: classes/Settings.php:1076
